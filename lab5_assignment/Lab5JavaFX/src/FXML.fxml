<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="Controller">
   <children>
      <Label fx:id="registration" layoutX="268.0" layoutY="57.0" text="Registration" />
      <Label fx:id="namelabel" layoutX="80.0" layoutY="106.0" text="Name" />
      <TextField fx:id="nameTextField" layoutX="226.0" layoutY="102.0" promptText="Enter your name" />
      <Button fx:id="submitbutton" layoutX="280.0" layoutY="317.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Label layoutX="82.0" layoutY="176.0" text="Age" />
      <Label layoutX="72.0" layoutY="241.0" text="Email id" />
      <TextField fx:id="ageTextField" layoutX="226.0" layoutY="172.0" promptText="Enter your age" />
      <TextField fx:id="emailTextField" layoutX="226.0" layoutY="237.0" promptText="Enter your email" />
   </children>
</AnchorPane>
